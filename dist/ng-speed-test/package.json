  "author": "Jeremy Quick",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "@angular/core": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "rxjs": "^7.5.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
