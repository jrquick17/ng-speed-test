{
  "name": "ng-speed-test",
  "version": "2.3.3",
  "readme": "README.md",
  "homepage": "https://ng-speed-test.jrquick.com",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "build:link": "npm run build && cd dist/ && npm link",
    "demo": "cd demo && ng serve",
    "demo:build": "cd demo && ng build --configuration production && cd ..",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng-speed-test -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "publish": "npm publish dist",
    "shipit": "npm run test && npm run build && npm run docs:build && npm run publish",
    "test": "ng test --browsers PhantomJS --progress --watch false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrquick17/ng-speed-test"
  },
  "author": {
    "name": "Jeremy Quick",
    "email": "me@jrquick.com",
    "url": "https://jrquick.com"
  },
  "keywords": [
    "angular",
    "internet-speed",
    "speed-test"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrquick17/ng-speed-test/issues"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/main.ts"
    }
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.12",
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-devkit/core": "^14.2.12",
    "@angular-devkit/schematics": "^14.2.12",
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^14.2.7",
    "@angular/cli": "^14.2.12",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@angular/service-worker": "^15.2.9",
    "@compodoc/compodoc": "^1.1.19",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "^3.10.7",
    "@types/node": "^12.20.55",
    "acorn": "^8.8.2",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "rollup": "^2.79.1",
    "rxjs": "^6.6.7",
    "ts-node": "^8.10.2",
    "tslib": "^2.5.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5",
    "webpack": "^5.75.0",
    "zone.js": "^0.11.8"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {},
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
