{
  "name": "ng-speed-test",
  "version": "0.0.1",
  "readme": "README.md",
  "homepage": "https://ng-speed-test.jrquick.com",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "demo": "cd demo && ng serve",
    "demo:build": "cd demo && npm install && ng build --prod && cd ..",
    "demo:test": "npm run build && cd demo && cp -fr ../dist/* node_modules/ng-speed-test/ && cd .. && npm run demo",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng-speed-test -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "publish": "npm publish dist",
    "shipit": "npm run lint && npm run test && npm run build && npm run demo:build && npm run docs:build && npm run publish",
    "test": "ng test --browsers PhantomJS --progress --watch false"
  },
  "ngPackage": {
    "lib": {
      "cssUrl": "inline",
      "entryFile": "src/main.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrquick17/ng-speed-test"
  },
  "author": {
    "name": "Jeremy Quick",
    "email": "me@jrquick.com",
    "url": "https://jrquick.com"
  },
  "keywords": [
    "angular",
    "internet-speed",
    "speed-test"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrquick17/ng-speed-test/issues"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.803.25",
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/core": "~8.3.25",
    "@angular-devkit/schematics": "~8.3.25",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.25",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/core-js": "^2.5.2",
    "@types/jasmine": "~3.5.3",
    "@types/node": "^13.7.0",
    "codelyzer": "~5.2.1",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "fsevents": "^2.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^5.7.1",
    "protractor": "~6.0.0",
    "rollup": "^1.31.0",
    "rxjs": "6.5.4",
    "ts-node": "~8.6.2",
    "tslint": "~5.6.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.5",
    "zone.js": "~0.9.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0"
  }
}
