{
  "name": "ng-speed-test",
  "version": "2.2.1",
  "readme": "README.md",
  "homepage": "https://ng-speed-test.jrquick.com",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "demo": "cd demo && ng serve",
    "demo:build": "cd demo && ng build --configuration production && cd ..",
    "demo:test": "npm run build && cd demo && cp -fr ../dist/* node_modules/ng-speed-test/ && cd .. && npm run demo",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng-speed-test -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "publish": "npm publish dist",
    "shipit": "npm run test && npm run build && npm run docs:build && npm run publish",
    "test": "ng test --browsers PhantomJS --progress --watch false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrquick17/ng-speed-test"
  },
  "author": {
    "name": "Jeremy Quick",
    "email": "me@jrquick.com",
    "url": "https://jrquick.com"
  },
  "keywords": [
    "angular",
    "internet-speed",
    "speed-test"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrquick17/ng-speed-test/issues"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/main.ts"
    }
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.13",
    "@angular-devkit/build-angular": "^12.2.13",
    "@angular-devkit/core": "^12.2.13",
    "@angular-devkit/schematics": "^12.2.13",
    "@angular/animations": "^12.2.13",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^12.2.13",
    "@angular/common": "^12.2.13",
    "@angular/compiler": "^12.2.13",
    "@angular/compiler-cli": "^12.2.13",
    "@angular/core": "^12.2.13",
    "@angular/forms": "^12.2.13",
    "@angular/language-service": "^12.2.13",
    "@angular/platform-browser": "^12.2.13",
    "@angular/platform-browser-dynamic": "^12.2.13",
    "@angular/router": "^12.2.13",
    "@angular/service-worker": "^12.2.13",
    "@compodoc/compodoc": "^1.1.15",
    "@types/core-js": "^2.5.5",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^12.20.37",
    "acorn": "^8.5.0",
    "codelyzer": "^6.0.2",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.8",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^12.2.5",
    "protractor": "~7.0.0",
    "rollup": "^2.59.0",
    "rxjs": "^6.6.7",
    "ts-node": "^8.10.2",
    "tslib": "^2.3.1",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack": "^5.64.0",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {},
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
